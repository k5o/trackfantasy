%header
  .container
    = link_to root_path, class: 'logo' do
      %span.track Track
      %span.fantasy Fantasy

    %ul.nav
      - if current_user
        %li= link_to 'Dashboard', '#'
        %li= link_to 'Account', user_path(current_user)
      - else
        %li= link_to 'Pricing/Signup', '#', class: 'js-cta'
        %li= link_to 'Login', login_path, class: 'login-link'
